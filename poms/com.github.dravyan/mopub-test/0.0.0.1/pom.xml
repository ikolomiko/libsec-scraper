<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.github.dravyan</groupId>
  <artifactId>mopub-test</artifactId>
  <version>0.0.0.1</version>
  <packaging>aar</packaging>
  <name>Mediation</name>
  <description>A application used as an example on how to set up pushing  its components to the Central Repository.</description>
  <url>https://github.com/dravyan/mopub-android-mediation</url>
  <licenses>
    <license>
      <name>MoPub SDK License Agreement</name>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>test</id>
      <name>test</name>
      <email>test@support.com</email>
    </developer>
  </developers>
  <scm>
    <connection>scm:git:https://github.com/dravyan/mopub-android-mediation.git</connection>
    <developerConnection>scm:git:https://github.com/dravyan/mopub-android-mediation.git</developerConnection>
    <url>https://github.com/dravyan/mopub-android-mediation</url>
  </scm>
  <dependencies>
    <dependency>
      <groupId>com.mopub</groupId>
      <artifactId>mopub-sdk</artifactId>
      <scope>compile</scope>
      <exclusions>
        <exclusion>
          <artifactId>libAvid-mopub</artifactId>
          <groupId>*</groupId>
        </exclusion>
        <exclusion>
          <artifactId>moat-mobile-app-kit</artifactId>
          <groupId>*</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.vungle</groupId>
      <artifactId>publisher-sdk-android</artifactId>
      <version>6.9.1</version>
      <scope>compile</scope>
    </dependency>
  </dependencies>
</project>
