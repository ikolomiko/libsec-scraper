<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.github.AlexTrotsenko</groupId>
  <artifactId>j2v8-debugger</artifactId>
  <version>0.2.2</version>
  <packaging>aar</packaging>
  <name>AlexTrotsenko/j2v8-debugger</name>
  <description>This project is an add-on for the excellent J2V8 Project.  It allows users to debug JS running in V8 using Chrome DevTools.  Uses Stetho for communication with Chrome DevTools.</description>
  <url>https://github.com/AlexTrotsenko/j2v8-debugger</url>
  <inceptionYear>2018</inceptionYear>
  <developers>
    <developer>
      <id>AlexTrotsenko</id>
      <name>Alex Trotsenko</name>
      <email></email>
    </developer>
  </developers>
  <scm>
    <connection>scm:git://github.com/AlexTrotsenko/j2v8-debugger.git</connection>
    <developerConnection>scm:git://github.com/AlexTrotsenko/j2v8-debugger.git</developerConnection>
    <url>git://github.com/AlexTrotsenko/j2v8-debugger.git</url>
  </scm>
  <dependencies>
    <dependency>
      <groupId>com.eclipsesource.j2v8</groupId>
      <artifactId>j2v8</artifactId>
      <version>6.1.0</version>
      <type>aar</type>
      <scope>compile</scope>
      <exclusions>
        <exclusion>
          <artifactId>*</artifactId>
          <groupId>*</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.facebook.stetho</groupId>
      <artifactId>stetho</artifactId>
      <version>1.5.1</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-stdlib-jdk7</artifactId>
      <version>1.3.72</version>
      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>androidx.annotation</groupId>
      <artifactId>annotation</artifactId>
      <version>1.1.0</version>
      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlinx</groupId>
      <artifactId>kotlinx-coroutines-core</artifactId>
      <version>1.3.5</version>
      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter</artifactId>
      <version>5.6.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.json</groupId>
      <artifactId>json</artifactId>
      <version>20190722</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>com.google.guava</groupId>
      <artifactId>guava</artifactId>
      <version>25.1-android</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>io.mockk</groupId>
      <artifactId>mockk</artifactId>
      <version>1.10.0</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
</project>
