<project
    xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.tensorflow</groupId>
  <artifactId>tensorflow-lite-task-vision</artifactId>
  <version>0.1.0</version>
  <packaging>aar</packaging>

  <name>TensorFlow Lite Vision Tasks</name>
  <url>https://www.tensorflow.org/mobile/tflite/</url>
  <description>Common text machine learning tasks based on TFLite</description>

  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>

  <developers>
    <developer>
      <name>TensorFlow Authors</name>
      <organization>TensorFlow</organization>
      <organizationUrl>http://tensorflow.org</organizationUrl>
    </developer>
  </developers>

  <scm>
    <connection>scm:git:git://github.com/tensorflow/tflite-support.git</connection>
    <developerConnection>scm:git:ssh://github.com:tensorflow/tflite-support.git</developerConnection>
    <url>http://github.com/tensorflow/tflite-support/releases/tag/v0.1.0</url>
   </scm>

  <build>
    <plugins>
      <plugin>
        <groupId>com.simpligility.maven.plugins</groupId>
        <artifactId>android-maven-plugin</artifactId>
        <version>4.1.0</version>
        <extensions>true</extensions>
        <configuration>
          <sign>
            <debug>false</debug>
          </sign>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>org.tensorflow</groupId>
      <artifactId>tensorflow-lite-task-base</artifactId>
      <version>0.1.0</version>
    </dependency>
    <dependency>
      <groupId>org.tensorflow</groupId>
      <artifactId>tensorflow-lite-support</artifactId>
      <version>0.1.0</version>
    </dependency>
  </dependencies>
</project>
