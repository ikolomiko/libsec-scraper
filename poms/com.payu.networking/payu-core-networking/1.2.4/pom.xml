<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.payu.networking</groupId>
  <artifactId>payu-core-networking</artifactId>
  <version>1.2.4</version>
  <packaging>aar</packaging>
  <name>PayU Core Networking SDK</name>
  <description>Core Networking library for Socket and Async requests</description>
  <url>https://github.com/payu-intrepos/payu-core-networking</url>
  <licenses>
    <license>
      <name>The MIT License</name>
      <url>https://opensource.org/licenses/MIT</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>payu</id>
      <name>payuindia</name>
      <email>payudev@payu.in</email>
    </developer>
  </developers>
  <scm>
    <connection>https://github.com/payu-intrepos/payu-core-networking.git</connection>
    <developerConnection>https://github.com/payu-intrepos/payu-core-networking.git</developerConnection>
    <url>https://github.com/payu-intrepos/payu-core-networking</url>
  </scm>
  <dependencies>
    <dependency>
      <groupId>com.android.support</groupId>
      <artifactId>appcompat-v7</artifactId>
      <version>27.1.0</version>
      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>io.socket</groupId>
      <artifactId>socket.io-client</artifactId>
      <version>0.8.3</version>
      <scope>runtime</scope>
      <exclusions>
        <exclusion>
          <artifactId>json</artifactId>
          <groupId>org.json</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.payu.payuanalytics</groupId>
      <artifactId>payuanalytics</artifactId>
      <version>1.1.1</version>
      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.12</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
</project>
