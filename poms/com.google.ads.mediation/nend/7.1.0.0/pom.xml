      <artifactId>play-services-ads</artifactId>
      <version>20.2.0</version>
    </dependency>
    <dependency>
      <groupId>net.nend.android</groupId>
      <artifactId>nend-sdk</artifactId>
      <version>7.1.0</version>
    </dependency>
