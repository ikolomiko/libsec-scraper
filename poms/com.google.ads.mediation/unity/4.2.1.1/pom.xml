<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.google.ads.mediation</groupId>
  <artifactId>unity</artifactId>
  <version>4.2.1.1</version>
  <packaging>aar</packaging>
  <name>UnityAds mediation adapter for the Google Mobile Ads SDK</name>
  <description>The UnityAds mediation adapter is a library that handles communication between the Google Mobile Ads SDK and the UnityAds SDK. It enables you to load ads from UnityAds using the mediation feature in the Google Mobile Ads SDK. This build does not contain the UnityAds SDK.</description>
  <url>https://developers.google.com/admob/android/mediation/unity</url>
  <licenses>
    <license>
      <name>Apache-2.0</name>
      <url>https://github.com/googleads/googleads-mobile-android-mediation/blob/main/LICENSE</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <dependencies>
    <dependency>
      <groupId>androidx.annotation</groupId>
      <artifactId>annotation</artifactId>
      <version>1.3.0</version>
    </dependency>
    <dependency>
      <groupId>com.google.android.gms</groupId>
      <artifactId>play-services-ads</artifactId>
      <version>21.0.0</version>
    </dependency>
  </dependencies>
</project>
