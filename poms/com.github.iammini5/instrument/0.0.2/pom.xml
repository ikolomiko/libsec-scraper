        <exclusion>
          <artifactId>javawriter</artifactId>
          <groupId>com.squareup</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.android.support.test.espresso</groupId>
      <artifactId>espresso-contrib</artifactId>
      <version>3.0.1</version>
