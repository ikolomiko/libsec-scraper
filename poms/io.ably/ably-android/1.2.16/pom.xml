  <url>https://www.github.com/ably/ably-java</url>
  <inceptionYear>2015</inceptionYear>
  <organization>
    <name>Ably</name>
    <url>https://ably.com/</url>
  </organization>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
