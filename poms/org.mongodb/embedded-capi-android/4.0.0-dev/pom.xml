<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.mongodb</groupId>
  <artifactId>embedded-capi-android</artifactId>
  <version>4.0.0-dev</version>
  <packaging>aar</packaging>
  <name>MongoDB Embedded for Java</name>
  <description>The MongoDB Embedded SDK for Java</description>
  <url>http://www.mongodb.org</url>
  <licenses>
    <license>
      <name>GNU Affero General Public License version 3</name>
      <url>http://www.gnu.org/licenses/agpl-3.0.html</url>
    </license>
  </licenses>
  <scm>
    <connection>scm:https://github.com/mongodb/mongo.git</connection>
