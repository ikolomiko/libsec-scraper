<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.tokeninc.httpdownloader</groupId>
  <artifactId>httpdownloader</artifactId>
  <version>1.0.0</version>
  <packaging>aar</packaging>
  <name>Token FTP Downloader</name>
  <description>Token HTTP Downloader</description>
  <url>https://github.com/tokeninc/TokenHTTPDownloader</url>
  <licenses>
    <license>
      <name>Apache-2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <scm>
    <connection>https://github.com/tokeninc/TokenHTTPDownloader.git</connection>
