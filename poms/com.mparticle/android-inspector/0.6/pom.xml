  <name>mParticle Inspector widget for observing SDK events.</name>
  <description>mParticle Inspector widget for observing SDK events.</description>
  <url>https://github.com/mparticle/mparticle-sdk-android</url>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>http://www.apache.org/license/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>mParticle</id>
      <name>mParticle Inc.</name>
      <email>dev@mparticle.com</email>
    </developer>
  </developers>
  <scm>
