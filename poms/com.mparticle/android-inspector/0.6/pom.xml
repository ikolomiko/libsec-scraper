      <scope>compile</scope>
      <exclusions>
        <exclusion>
          <artifactId>*</artifactId>
          <groupId>*</groupId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>com.mparticle</groupId>
      <artifactId>android-core</artifactId>
      <version>[5.8.0, )</version>
