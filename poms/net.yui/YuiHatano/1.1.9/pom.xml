  <packaging>aar</packaging>
  <name>YuiHatano</name>
  <description>Android DAO单元测试框架</description>
  <url>https://github.com/kkmike999/YuiHatano</url>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>kkmike999</id>
