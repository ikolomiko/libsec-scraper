<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>io.github.nhsconnect</groupId>
  <artifactId>fido-uaf-client-android</artifactId>
  <version>1.0.3</version>
  <packaging>aar</packaging>
  <name>FIDO UAF (Universal Authentication Framework) client for Android</name>
  <description>You can use the FIDO UAF client to authenticate logging in to Android apps without a password.</description>
  <url>https://github.com/nhsconnect/nhsapp-fido-client-android</url>
  <licenses>
    <license>
      <name>MIT</name>
      <url>https://github.com/nhsconnect/nhsapp-fido-client-android/blob/master/LICENSE</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <id>nhsapp</id>
      <name>NHS App</name>
      <email>nhsapp@nhs.net</email>
    </developer>
  </developers>
  <scm>
    <connection>scm:git:https://github.com/nhsconnect/nhsapp-fido-client-android.git</connection>
    <developerConnection>scm:git:https://github.com/nhsconnect/nhsapp-fido-client-android.git</developerConnection>
    <url>https://github.com/nhsconnect/nhsapp-fido-client-android</url>
  </scm>
  <dependencies>
    <dependency>
      <groupId>com.android.support</groupId>
      <artifactId>appcompat-v7</artifactId>
      <version>27.1.1</version>
      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>org.mindrot</groupId>
      <artifactId>jbcrypt</artifactId>
      <version>0.4</version>
      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>com.google.code.gson</groupId>
      <artifactId>gson</artifactId>
      <version>2.8.5</version>
      <scope>runtime</scope>
    </dependency>
    <dependency>
      <groupId>org.jetbrains.kotlin</groupId>
      <artifactId>kotlin-stdlib-jdk7</artifactId>
      <version>1.3.11</version>
      <scope>runtime</scope>
    </dependency>
  </dependencies>
</project>
